package training;

public class DeliveredState implements PackageState {

	@Override
	public void prev(Package pkg) {
		pkg.prev(new ReceivedState());		
	}

	@Override
	public void next(Package pkg) {
				
	}

	@Override
	public void printStatus() {
		System.out.println(" ");		
	}	
	
	@Override 
	public String toString() {
		return "DeliveredState{}" ; 
	}

}
